@model RPBDIS_4.Models.Equipment

<h2>Equipment Details</h2>

<table class="table">
    <tr>
        <th>Inventory Number</th>
        <td>@Model.InventoryNumber</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Start Date</th>
        <td>@Model.StartDate?.ToString("d")</td>
    </tr>
    <tr>
        <th>Location</th>
        <td>@Model.Location</td>
    </tr>
</table>

<a asp-action="Index">Back to List</a>
